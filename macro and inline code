#include <iostream>
using namespace std;
#define SQUARE(x)x*x
inline int square(int x)
{
    return x*x;
}
int main()
{
    int a=5;
    int macroResult=SQUARE(a+1);
    cout<<"Macro Result: "<<macroResult<<endl;
    int inlineResult=square(a+1);
    cout<<"Inline Function Result: "<<inlineResult<<endl;
    return 0;
}
